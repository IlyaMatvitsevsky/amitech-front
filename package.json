{
  "name": "amitech-sotefront",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "cross-env ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "prettier-fix": "prettier --write .",
    "generate:definitions": "cross-env DEBUG=1 DOTENV_CONFIG_PATH=.env.local graphql-codegen -r dotenv/config --config codegen.yml"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.3",
    "@next/bundle-analyzer": "^13.0.5",
    "@svgr/webpack": "^6.4.0",
    "antd": "^5.0.1",
    "clsx": "^1.2.1",
    "next": "12.3.0",
    "next-compose-plugins": "^2.2.1",
    "next-plausible": "^3.6.4",
    "node-fetch": "^3.2.10",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intersection-observer": "^9.4.1",
    "react-transition-group": "^4.4.5",
    "sharp": "^0.31.2",
    "source-map-explorer": "^2.5.3",
    "swiper": "^8.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@graphql-codegen/cli": "^2.13.4",
    "@graphql-codegen/introspection": "^2.2.1",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@graphql-codegen/typescript-react-apollo": "^3.3.3",
    "@types/lodash.omit": "^4.5.7",
    "@types/lodash.pick": "^4.4.7",
    "@types/lodash.random": "^3.2.7",
    "@types/lodash.throttle": "^4.1.7",
    "@types/node": "18.7.16",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "@types/react-transition-group": "^4.4.5",
    "autoprefixer": "^10.4.8",
    "babel-loader": "^8.2.5",
    "cross-env": "^7.0.3",
    "eslint": "8.23.0",
    "eslint-config-next": "12.3.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "graphql": "^16.6.0",
    "lint-staged": "^13.0.3",
    "path": "^0.12.7",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.8",
    "ts-loader": "^9.4.1",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "4.8.3"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint",
      "prettier --write",
      "git add"
    ],
    "**/*.{md,mdx,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "next-unused": {
    "alias": {
      "@lib/*": [
        "lib/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@utils/*": [
        "utils/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "lib",
      "pages"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  },
  "resolutions": {
    "webpack": "^5"
  }
}
